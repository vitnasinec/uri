{
    "name": "vitnasinec/uri",
    "description": "",
    "keywords": [
        "vitnasinec",
        "uri"
    ],
    "homepage": "https://github.com/vitnasinec/uri",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "Vit Nasinec",
            "email": "vitnasinec@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.2|^8.0",
        "illuminate/http": "^6.0|^7.0|^8.0|^9.0",
        "illuminate/routing": "^6.0|^7.0|^8.0|^9.0",
        "illuminate/support": "^6.0|^7.0|^8.0|^9.0"
    },
    "require-dev": {
        "ext-json": "*",
        "orchestra/testbench": "^4.0|^5.0|^6.0",
        "phpunit/phpunit": "^8.0|^9.0"
    },
    "autoload": {
        "psr-4": {
            "Vitnasinec\\Uri\\": "src"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Vitnasinec\\Uri\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Vitnasinec\\Uri\\UriServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
